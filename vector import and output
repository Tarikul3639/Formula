#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define nn '\n'
#define EPS 1e-9
#define PI 3.1415926535897932384626433832795
int main()
{
    int row;
    int col;
    cout << "Enter row number = ";
    cin >> row;
    cout << "Enter colum number = ";
    cin >> col;
    vector<vector<int>> v;
    int element;
    for (int i = 0; i < row; i++)
    {
        vector<int> v2;
        for (int j = 0; j < col; j++)
        {

            cin >> element;
            v2.push_back(element);
        }
        v.push_back(v2);
    }
    cout << "Matrix = " << nn;
    for (int i = 0; i < row; i++)
    {
        for (int j = 0; j < col; j++)
        {
            cout << v[i][j] << " ";
        }
        cout << nn;
    }
}
