#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define nn '\n'
#define EPS 1e-9
#define PI 3.1415926535897932384626433832795
#define pb push_back
#define asort(a) sort(a.begin(), a.end())
#define areverse(a) reverse(a.begin(), a.end())
#define sp " "
#define no cout << "NO" << nn
#define yes cout << "YES" << nn
#define test  \
    ll t;     \
    cin >> t; \
    while (t--)
int main()
{
    map<string, int> mp;
    int n;
    int c = 0;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        string s;
        cin >> s;
        if (mp[s] == 0) no;

        else yes;
         mp[s]++;
    }

    return 0;
}
